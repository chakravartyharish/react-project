{
  "name": "minesweeper-react",
  "private": true,
  "version": "0.0.0",
  "description": "A classic Minesweeper game built with React",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "mcp:filesystem": "npx -y @modelcontextprotocol/server-filesystem /home/harish/Development/minesweeper-react",
    "mcp:git": "npx -y @modelcontextprotocol/server-git --repository /home/harish/Development/minesweeper-react",
    "mcp:sqlite": "npx -y @modelcontextprotocol/server-sqlite ./dev.db",
    "mcp:github": "GITHUB_PERSONAL_ACCESS_TOKEN=$GITHUB_TOKEN npx -y @modelcontextprotocol/server-github",
    "mcp:brave": "BRAVE_API_KEY=$BRAVE_API_KEY npx -y @modelcontextprotocol/server-brave-search",
    "mcp:test": "echo 'Testing MCP servers...' && npm run mcp:filesystem -- --help && npm run mcp:git -- --help",
    "mcp:browser": "npx -y @modelcontextprotocol/server-playwright",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "vite": "^7.1.2",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^3.2.4"
  }
}
